{
  "name": "flutter-studio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "test": "yarn test:unit",
    "test:unit": "cross-env BABEL_ENV=test jest --coverage --no-cache",
    "test:watch": "yarn test:unit -- --watch",
    "test:coverage": "yarn test:unit -- --coverage"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@material-ui/core": "^4.9.7",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.46",
    "@material-ui/pickers": "^3.2.10",
    "autoprefixer": "^9.7.4",
    "clsx": "^1.1.0",
    "framer-motion": "^1.10.0",
    "html5-device-mockups": "^3.2.1",
    "lodash": "^4.17.15",
    "mobile-detect": "^1.4.4",
    "next": "9.3.1",
    "next-redux-wrapper": "^5.0.0",
    "normalize.css": "^8.0.1",
    "perfect-scrollbar": "^1.5.0",
    "postcss-import": "^12.0.1",
    "prism-themes": "^1.3.0",
    "prismjs": "^1.19.0",
    "prop-types": "^15.7.2",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-icons": "^3.9.0",
    "react-redux": "^7.2.0",
    "react-responsive": "^8.0.3",
    "react-select": "^3.1.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
